algoritmo "semnome"
// se ... fimse
// se ... senao
// escolha ... caso
var
  idade: inteiro

inicio
  escreva ("por gentileza digite sua idade")
  leia (idade)
  se idade >= 18 entao
  escreva ("você é maior de idade")
  senao
  escreva ("você é menor de idade")
  fimse




fimalgoritmo
